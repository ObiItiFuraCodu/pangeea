    <EditText
        android:id="@+id/email2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/rounded3"
        android:drawableLeft="@drawable/vector_user"
        android:drawablePadding="10dp"
        android:elevation="8dp"
        android:ems="10"
        android:fontFamily="@font/poppins_bold"
        android:hint="Email"
        android:inputType="textPersonName"
        android:paddingLeft="16dp"
        android:textColor="#C8C8C8"
        android:textColorHint="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/guideline276"
        app:layout_constraintEnd_toStartOf="@+id/guideline200"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guideline199"
        app:layout_constraintTop_toTopOf="@+id/guideline275" />

    <EditText
