    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline172"
        app:layout_constraintEnd_toStartOf="@+id/guideline170"
        app:layout_constraintStart_toStartOf="@+id/guideline169"
        app:layout_constraintTop_toTopOf="@+id/guideline6"
        android:background="@drawable/roundedmock"
        android:elevation="10dp">

        <ListView
            android:id="@+id/submission_list"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline451"
            app:layout_constraintEnd_toStartOf="@+id/guideline453"
            app:layout_constraintStart_toStartOf="@+id/guideline454"
            app:layout_constraintTop_toTopOf="@+id/guideline452" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline454"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.04"
            app:layout_constraintStart_toStartOf="parent" />

