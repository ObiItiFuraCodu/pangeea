        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline164"
        app:layout_constraintEnd_toStartOf="@+id/guideline170"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/guideline169"
        app:layout_constraintTop_toTopOf="@+id/guideline7"
        app:layout_constraintVertical_bias="1.0"
        android:background="@drawable/roundedmock"
        android:elevation="10dp">

        <ListView
            android:id="@+id/support_lesson_list"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline450"
            app:layout_constraintEnd_toStartOf="@+id/guideline448"
            app:layout_constraintStart_toStartOf="@+id/guideline443"
            app:layout_constraintTop_toTopOf="@+id/guideline449" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline443"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
